import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        System.out.printf("Bird said:"+a);
        System.out.printf("\nAlice must go in path-"+((a%10)+(a/10)));        
}
}
