import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=sc.nextInt();
        if((a<=c)&&(b<=c))
           {
            System.out.println("c is largest then a and b");
           }
        else if
            ((c<=b)&&(a<=b))
        {
                        System.out.println("b is largest then a and c");

        }
    }
}
